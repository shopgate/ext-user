{
  "version": "2.2.2",
  "id": "@shopgate/user",
  "trusted": true,
  "configuration": {
    "countryCodes": {
      "type": "static",
      "destination": "frontend",
      "params": {
        "value": "DE,AT,FR,GB"
      }
    }
  },
  "components": [
    {
      "id": "RegisterRoute",
      "path": "frontend/Register/route.jsx",
      "target": "app.routes",
      "type": "portals"
    },
    {
      "id": "CheckoutAddAddressRoute",
      "path": "frontend/Checkout/AddAddress/route.jsx",
      "target": "app.routes",
      "type": "portals"
    },
    {
      "id": "SelectAddressRoute",
      "path": "frontend/Checkout/SelectAddress/route.jsx",
      "target": "app.routes",
      "type": "portals"
    },
    {
      "id": "CheckoutShipping",
      "path": "frontend/Checkout/ShippingAddress/index.jsx",
      "target": "checkout.shipping_address",
      "type": "portals"
    },
    {
      "id": "CheckoutBilling",
      "path": "frontend/Checkout/BillingAddress/index.jsx",
      "target": "checkout.billing_address",
      "type": "portals"
    },
    {
      "type": "subscribers",
      "id": "CheckoutSubscribers",
      "path": "frontend/Checkout/subscribers.js"
    },
    {
      "id": "RegisterSubscription",
      "path": "frontend/Register/subscribers.js",
      "type": "subscribers"
    },
    {
      "id": "en-US",
      "path": "frontend/locale/en-US.json",
      "type": "translations"
    },
    {
      "id": "de-DE",
      "path": "frontend/locale/de-DE.json",
      "type": "translations"
    }
  ]
}
