{
  "version": "2.2.3-beta.8",
  "id": "@shopgate/user",
  "trusted": true,
  "configuration": {
    "countryCodes": {
      "type": "static",
      "destination": "frontend",
      "params": {
        "value": "DE,AT,FR,GB"
      }
    },
    "addressFields": {
      "type": "static",
      "destination": "frontend",
      "params": {
        "value": ["firstName", "lastName", "street1", "city", "country", "zipCode"]
      }
    },
    "splitDefaultAddressesByTags": {
      "type": "static",
      "destination": "frontend",
      "params": {
        "value": ["default"]
      }
    }
  },
  "components": [
    {
      "id": "AddressBookRoute",
      "path": "frontend/pages/AddressBook/Overview/route.jsx",
      "target": "app.routes",
      "type": "portals"
    },
    {
      "id": "AddressBookAddRoute",
      "path": "frontend/pages/AddressBook/AddAddress/route.jsx",
      "target": "app.routes",
      "type": "portals"
    },
    {
      "id": "UserSubscribers",
      "path": "frontend/subscriptions/index.js",
      "type": "subscribers"
    },
    {
      "id": "UserReducers",
      "path": "frontend/reducers/index.js",
      "type": "reducers"
    },
    {
      "id": "RegisterRoute",
      "path": "frontend/Register/route.jsx",
      "target": "app.routes",
      "type": "portals"
    },
    {
      "id": "CheckoutAddAddressRoute",
      "path": "frontend/pages/Checkout/AddAddress/route.jsx",
      "target": "app.routes",
      "type": "portals"
    },
    {
      "id": "SelectAddressRoute",
      "path": "frontend/pages/Checkout/SelectAddress/route.jsx",
      "target": "app.routes",
      "type": "portals"
    },
    {
      "id": "CheckoutShipping",
      "path": "frontend/pages/Checkout/ShippingAddress/index.jsx",
      "target": "checkout.shipping_address",
      "type": "portals"
    },
    {
      "id": "CheckoutBilling",
      "path": "frontend/pages/Checkout/BillingAddress/index.jsx",
      "target": "checkout.billing_address",
      "type": "portals"
    },
    {
      "_comment": "Remove default 'my account' menu entries from the themes.",
      "id": "Empty",
      "path": "frontend/components/Empty.jsx",
      "target": "nav-menu.my-account",
      "type": "portals"
    },
    {
      "_comment": "Add a simple Logout entry to the end of the respective menu.",
      "id": "LogoutMenuEntry",
      "path": "frontend/components/LogoutMenuEntry/index.jsx",
      "target": "nav-menu.content.after",
      "type": "portals"
    },
    {
      "_comment": "Add the new user menu.",
      "id": "UserMenu",
      "path": "frontend/components/UserMenu/index.jsx",
      "target": "nav-menu.store-information.before",
      "type": "portals"
    },
    {
      "type": "subscribers",
      "id": "CheckoutSubscribers",
      "path": "frontend/Checkout/subscribers.js"
    },
    {
      "id": "RegisterSubscription",
      "path": "frontend/Register/subscribers.js",
      "type": "subscribers"
    },
    {
      "id": "en-US",
      "path": "frontend/locale/en-US.json",
      "type": "translations"
    },
    {
      "id": "de-DE",
      "path": "frontend/locale/de-DE.json",
      "type": "translations"
    },
    {
      "id": "fr-FR",
      "path": "frontend/locale/fr-FR.json",
      "type": "translations"
    },
    {
      "id": "nl-NL",
      "path": "frontend/locale/nl-NL.json",
      "type": "translations"
    }
  ]
}
