{
  "version": "3.0.0-rc.1",
  "id": "@shopgate/user",
  "trusted": true,
  "configuration": {
    "addressForm": {
      "type": "admin",
      "destination": "backend",
      "default": {
        "fields": {
          "firstName": {
            "sortOrder": 1,
            "label": "* First name",
            "type": "text",
            "required": true,
            "visible": true
          },
          "lastName": {
            "sortOrder": 3,
            "label": "* Last name",
            "type": "text",
            "required": true,
            "visible": true
          },
          "street1": {
            "sortOrder": 4,
            "label": "* Street and number",
            "type": "text",
            "required": true,
            "visible": true
          },
          "street2": {
            "sortOrder": 5,
            "label": "Street 2",
            "type": "text",
            "required": false,
            "actions": [{
              "type": "setVisibility",
              "rules": [{
                "context": "street1",
                "type": "notIn",
                "data": [""]
              }]
            }]
          },
          "zipCode": {
            "sortOrder": 6,
            "label": "* Postal code",
            "type": "text",
            "required": true,
            "visible": true
          },
          "city": {
            "sortOrder": 7,
            "label": "* City",
            "type": "text",
            "required": true,
            "visible": true
          },
          "country": {
            "sortOrder": 8,
            "label": "* Country",
            "type": "country",
            "default": "US",
            "countries": ["DE", "US", "AT", "FR", "GB"],
            "required": true,
            "visible": true
          },
          "province": {
            "sortOrder": 9,
            "label": "Province",
            "type": "province",
            "required": false,
            "actions": [{
              "type": "setVisibility",
              "rules": [{
                "context": "country",
                "type": "oneOf",
                "data": ["US", "AT", "FR", "GB"]
              }]
            }]
          }
        }
      },
      "params": {
        "type": "json",
        "label": "Address fields",
        "required": true
      }
    },
    "addressDefaultGroups": {
      "type": "admin",
      "destination": "backend",
      "default": ["shipping", "billing"],
      "params": {
        "type": "json",
        "label": "Address default groups",
        "required": true,
        "options": {
          "placeholder": "[\"shipping\", \"billing\"]"
        }
      }
    },
    "userMenuEntries": {
      "type": "admin",
      "destination": "backend",
      "default": {
        "accountProfile": true,
        "addressBook": true
      },
      "params": {
        "type": "json",
        "label": "User menu entries",
        "required": true,
        "options": {
          "placeholder": "{\"accountProfile\": true, \"addressBook\": true}"
        }
      }
    }
  },
  "components": [
    {
      "id": "RegisterPage",
      "path": "frontend/pages/Register/index.jsx",
      "target": "app.routes",
      "type": "portals"
    },
    {
      "id": "MyProfilePage",
      "path": "frontend/pages/MyProfile/index.jsx",
      "target": "app.routes",
      "type": "portals"
    },
    {
      "id": "PasswordPage",
      "path": "frontend/pages/ChangePassword/index.jsx",
      "target": "app.routes",
      "type": "portals"
    },
    {
      "id": "AddressBookPage",
      "path": "frontend/pages/AddressBook/Overview/index.jsx",
      "target": "app.routes",
      "type": "portals"
    },
    {
      "id": "AddressDetailsPage",
      "path": "frontend/pages/AddressBook/AddressDetails/index.jsx",
      "target": "app.routes",
      "type": "portals"
    },
    {
      "id": "UserSubscribers",
      "path": "frontend/subscriptions/index.js",
      "type": "subscribers"
    },
    {
      "id": "UserReducers",
      "path": "frontend/reducers/index.js",
      "type": "reducers"
    },
    {
      "id": "CheckoutShipping",
      "path": "frontend/pages/Checkout/ShippingAddress/index.jsx",
      "target": "checkout.shipping_address",
      "type": "portals"
    },
    {
      "id": "CheckoutBilling",
      "path": "frontend/pages/Checkout/BillingAddress/index.jsx",
      "target": "checkout.billing_address",
      "type": "portals"
    },
    {
      "id": "UserMenu",
      "path": "frontend/components/UserMenu/index.jsx",
      "target": "nav-menu.store-information.before",
      "type": "portals"
    },
    {
      "id": "Empty",
      "path": "frontend/components/Empty.jsx",
      "target": "nav-menu.my-account.header",
      "type": "portals"
    },
    {
      "id": "en-US",
      "path": "frontend/locale/en-US.json",
      "type": "translations"
    },
    {
      "id": "de-DE",
      "path": "frontend/locale/de-DE.json",
      "type": "translations"
    },
    {
      "id": "fr-FR",
      "path": "frontend/locale/fr-FR.json",
      "type": "translations"
    },
    {
      "id": "nl-NL",
      "path": "frontend/locale/nl-NL.json",
      "type": "translations"
    }
  ]
}
